=== tests/cases/compiler/moduleMerge.ts ===
// This should not compile both B classes are in the same module this should be a collission

module A
>A : A
{
    class B
>B : B
    {
        public Hello(): string
>Hello : () => string
        {
            return "from private B";
        }
    }
}

module A
>A : A
{
    export class B
>B : B
    {
        public Hello(): string
>Hello : () => string
        {
            return "from export B";
        }
    }
}
